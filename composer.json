{
	"name": "lentille/symfony-bundle",
	"type": "symfony-bundle",
	"license": "MIT",
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": ">=8.1",
		"doctrine/orm": "^2.13",
		"symfony/form": "^6.1",
		"symfony/framework-bundle": "^6.1",
		"symfony/serializer": "^6.1",
		"symfony/translation-contracts": "^3.0"
	},
	"require-dev": {
		"symfony/security-core": "^6.1",
		"twig/twig": "^3.4"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Lentille\\SymfonyBundle\\": "src/"
		}
	}
}
